let ServicetypeComponent = class ServicetypeComponent {
    constructor(fb, toastrService, servicetypeservice, modalService) {
        this.fb = fb;
        this.toastrService = toastrService;
        this.servicetypeservice = servicetypeservice;
        this.modalService = modalService;
        this.type = 'list';
    }
    ngOnInit() {
        this.getServiceTypes();
        //   this.getUoms();
        this.frmServicetype = this.fb.group({
            id: null,
            servicetypeId: null,
            servicetypeCode: [null, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(100)])],
            servicetypeName: [null, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(50)])],
            dateCreated: null
        });
    }
    getServiceTypes() {
        this.servicetypeservice.getServicetypes().subscribe(servicetypes => this.servicetypes = servicetypes);
    }
    addServicetype(servicetype) {
        // console.log(service);
        this.servicetypeservice.addServicetype(servicetype).subscribe(servicetype => {
            this.getServiceTypes();
        });
    }
    updateServicetype(servicetype) {
        this.servicetypeservice.updateServicetype(servicetype).subscribe(servicetype => {
            this.getServiceTypes();
        });
    }
    toggle(type) {
        this.type = type;
    }
    openModal(modalContent, servicetype) {
        //console.log(service);
        if (servicetype) {
            this.servicetype = servicetype;
            this.frmServicetype.setValue(servicetype);
        }
        else {
            this.servicetype = new _servicetype_model__WEBPACK_IMPORTED_MODULE_5__["Servicetype"]();
        }
        this.modalRef = this.modalService.open(modalContent, { container: '.app' });
        this.modalRef.result.then((result) => {
            this.frmServicetype.reset();
        }, (reason) => {
            this.frmServicetype.reset();
        });
    }
    closeModal() {
        this.modalRef.close();
    }
    onSubmit(servicetype) {
        console.log(servicetype);
        if (this.frmServicetype.valid) {
            if (servicetype.id) {
                this.updateServicetype(servicetype);
            }
            else {
                this.addServicetype(servicetype);
            }
            this.modalRef.close();
        }
    }
};
