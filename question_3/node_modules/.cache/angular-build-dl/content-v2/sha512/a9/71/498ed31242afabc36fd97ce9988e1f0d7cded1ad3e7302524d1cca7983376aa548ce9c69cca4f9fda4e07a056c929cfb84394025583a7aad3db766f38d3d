/* harmony import */ var _customer_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ./customer.service */ "./src/app/pages/customer/customer.service.ts");
/* harmony import */ var _theme_components_menu_menu_service__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! ../../theme/components/menu/menu.service */ "./src/app/theme/components/menu/menu.service.ts");








let CustomerComponent = class CustomerComponent {
    constructor(fb, toastrService, clientService, menuService, modalService) {
        this.fb = fb;
        this.toastrService = toastrService;
        this.clientService = clientService;
        this.menuService = menuService;
        this.modalService = modalService;
        this.type = 'list';
        this.genders = ['male', 'female'];
        this.menuSelectSettings = {
            enableSearch: true,
            checkedStyle: 'fontawesome',
            buttonClasses: 'btn btn-secondary btn-block',
            dynamicTitleMaxItems: 0,
            displayAllSelectedText: true,
            showCheckAll: true,
            showUncheckAll: true
        };
        this.menuSelectTexts = {
            checkAll: 'Select all',
            uncheckAll: 'Unselect all',
            checked: 'menu item selected',
            checkedPlural: 'menu items selected',
            searchPlaceholder: 'Find menu item...',
            defaultTitle: 'Select menu items for user',
            allSelected: 'All selected',
        };
        this.menuSelectOptions = [];
        this.menuItems = this.menuService.getVerticalMenuItems();
        this.menuItems.forEach(item => {
            const menu = {
                id: item.id,
                name: item.title
            };
            this.menuSelectOptions.push(menu);
        });
    }
    ngOnInit() {
        this.getCountries();
        this.getCounties();
        this.getClients();
        this.frmClient = this.fb.group({
            id: null,
            fullName: [null, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(100)])],
            documentNumber: [null, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(50)])],
            mobileNumber: null,
            active: true,
            status: true,
            firstname: '',
            displayName: '',
            imagePresent: '',
            middleName: '',
            lastName: '',
            accountNo: '',
            createdOn: '',
            country: 0,
            county: 0,
            gender: '',
            accountActivated: true,
            email: '',
            address: '',
            imageId: '',
        });
    }
    getClients() {
        this.clientService.getClients().subscribe(clients => this.clients = clients);
    }
    getCountries() {
        this.clientService.getCountries().subscribe(countries => this.countries = countries);
    }
    getCounties() {
        this.clientService.getCounties().subscribe(counties => this.counties = counties);
    }
    addClient(client) {
        this.clientService.addClient(client).subscribe(client => {
            this.getClients();
        });
    }
    updateClient(client) {
        this.clientService.updateClient(client).subscribe(client => {
            this.getClients();
        });
    }
    deleteClient(client) {
        this.clientService.deleteClient(client.id).subscribe(result => this.getClients());
    }
    toggle(type) {
        this.type = type;
    }
    openMenuAssign(event) {
        let parent = event.target.parentNode;
        while (parent) {
            parent = parent.parentNode;
            if (parent.classList.contains('content')) {
                parent.classList.add('flipped');
                parent.parentNode.parentNode.classList.add('z-index-1');
                break;
            }
        }
    }
    closeMenuAssign(event) {
        let parent = event.target.parentNode;
        while (parent) {
            parent = parent.parentNode;
            if (parent.classList.contains('content')) {
                parent.classList.remove('flipped');
                parent.parentNode.parentNode.classList.remove('z-index-1');
                break;
            }
        }
    }
    assignMenuItemsToUser(user) {
        this.updateClient(user);
        sessionStorage.setItem('userMenuItems', JSON.stringify(user.menuIds));
        this.toastrService.success('Please, logout and login to see result.', 'Successfully assigned !');
    }
    openModal(modalContent, client) {
        if (client) {
            this.client = client;
            this.frmClient.setValue(client);
            // this.genderOption = user.profile.gender;
        }
        else {
            this.client = new _customer_model__WEBPACK_IMPORTED_MODULE_5__["Client"]();
            // this.user.profile = new UserProfile();
            // this.user.work = new UserWork();
            // this.user.contacts = new UserContacts();
            // this.user.social = new UserSocial();
            // this.user.settings = new UserSettings();
        }
        this.modalRef = this.modalService.open(modalContent, { container: '.app', scrollable: true });
        this.modalRef.result.then((result) => {
            this.frmClient.reset();
            // this.genderOption = null;
        }, (reason) => {
            this.frmClient.reset();
            // this.genderOption = null;
        });
    }
    closeModal() {
        this.modalRef.close();
    }
    onSubmit(client) {
        if (this.frmClient.valid) {
            if (client.id) {
                this.updateClient(client);
            }
            else {
                this.addClient(client);
            }
            this.modalRef.close();
        }
    }
};
CustomerComponent.ctorParameters = () => [
