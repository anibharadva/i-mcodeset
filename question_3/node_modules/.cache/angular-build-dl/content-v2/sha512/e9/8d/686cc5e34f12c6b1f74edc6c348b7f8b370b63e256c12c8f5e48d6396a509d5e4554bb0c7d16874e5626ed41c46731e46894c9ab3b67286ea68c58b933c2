            case 'Germany':
                return `<span class="flag-icon flag-icon-de mr-2"></span>${c.label}`;
            case 'France':
                return `<span class="flag-icon flag-icon-fr mr-2"></span>${c.label}`;
            case 'Great Britain':
                return `<span class="flag-icon flag-icon-gb mr-2"></span>${c.label}`;
            default:
                return c.label;
        }
