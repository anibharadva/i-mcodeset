let ImageUploaderComponent = class ImageUploaderComponent {
    fileChange(input) {
        const reader = new FileReader();
        if (input.files.length) {
            const file = input.files[0];
            reader.onload = () => {
                this.image = reader.result;
            };
            reader.readAsDataURL(file);
        }
    }
    removeImage() {
        this.image = '';
    }
};
