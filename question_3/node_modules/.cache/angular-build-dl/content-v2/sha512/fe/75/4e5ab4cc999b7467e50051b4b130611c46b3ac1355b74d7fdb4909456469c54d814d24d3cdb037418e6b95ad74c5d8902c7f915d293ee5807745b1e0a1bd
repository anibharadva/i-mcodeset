/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/fesm2015/core.js");
/* harmony import */ var _angular_forms__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/forms */ "./node_modules/@angular/forms/fesm2015/forms.js");
/* harmony import */ var _customer_customer_service__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ../../customer/customer.service */ "./src/app/pages/customer/customer.service.ts");
/* harmony import */ var _admission_service__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ./admission.service */ "./src/app/pages/mortuary/admission/admission.service.ts");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! @angular/router */ "./node_modules/@angular/router/fesm2015/router.js");
/* harmony import */ var ngx_toastr__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ngx-toastr */ "./node_modules/ngx-toastr/fesm2015/ngx-toastr.js");
/* harmony import */ var _services_service_service__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! ../../services/service.service */ "./src/app/pages/services/service.service.ts");








let AdmissionComponent = class AdmissionComponent {
    constructor(formBuilder, clientService, mileleService, admissionService, router, toastrService) {
        this.formBuilder = formBuilder;
        this.clientService = clientService;
        this.mileleService = mileleService;
        this.admissionService = admissionService;
        this.router = router;
        this.toastrService = toastrService;
        this.nextOfKin = [];
        this.invoiceServices = [];
        this.details = {};
        this.showDetails = false; // hidden by default
        this.showList = true;
        this.showInvoice = false;
        // Datepicker in a popup
        this.modelPopup = { year: new Date().getFullYear(), month: new Date().getMonth() + 1, day: new Date().getDate() - 3 };
    }
    ngOnInit() {
        this.dayTypes = [
            { id: 1, dayType: 'WeekDay' },
            { id: 2, dayType: 'WeekEnd' }
        ];
        this.getAdmissions();
        this.getClients();
        this.getCountries();
        this.getCounties();
        this.getServices();
        this.steps = [
            { name: 'Client Details', icon: 'fa-lock', active: true, valid: false, hasError: false },
            { name: 'Deceased Details', icon: 'fa-user', active: false, valid: false, hasError: false },
            { name: 'Next of Kin', icon: 'fa-credit-card', active: false, valid: false, hasError: false },
            { name: 'Confirm Details & Submit', icon: 'fa-check-square-o', active: false, valid: false, hasError: false }
        ];
        this.clientForm = this.formBuilder.group({
            clientId: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required]
        });
        this.personalForm = this.formBuilder.group({
            'causeOfDeath': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'dateOfBirth': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'dateOfDeath': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'gender': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'description': null,
            'documentNumber': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'fullName': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'imageId': null,
            country: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            county: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            fridge: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            service: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required]
        });
        this.nextOfKinForm = this.formBuilder.group({
            'address': [''],
            'admissionId': null,
            country: 0,
            county: 0,
            'documentNumber': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'email': [''],
            'fullName': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            'gender': [''],
            'id': null,
            'imageId': [''],
            'mobileNumber': ['']
        });
        this.invoiceFrom = this.formBuilder.group({
            'invoiceNo': null,
            'invoiceDate': null,
            'clientName': null,
            'documentNumber': null,
            'serviceName': null,
            'price': null,
            'fridgeName': null,
            'fridgePrice': null,
            'total': null
        });
    }
    toggleShow() {
        this.showList = false;
        this.showDetails = true;
    }
    showClientMaster() {
        this.router.navigateByUrl('/customer');
    }
    // Datepicker in a popup
    getClients() {
        this.clientService.getClients().subscribe(clients => this.clients = clients);
    }
    getCountries() {
        this.clientService.getCountries().subscribe(countries => this.countries = countries);
    }
    getCounties() {
        this.clientService.getCounties().subscribe(counties => this.counties = counties);
    }
    getServices() {
        this.mileleService.getServices().subscribe(services => this.services = services);
    }
    getFridgesByServiceId(id) {
        this.admissionService.getFridgesByServiceId(id).subscribe(fridges => this.fridges = fridges);
    }
    getAdmissions() {
        this.admissionService.getAdmissions().subscribe(admissions => this.admissions = admissions);
    }
    next() {
        const clientForm = this.clientForm;
        const personalForm = this.personalForm;
        const nextOfKinForm = this.nextOfKinForm;
        if (this.steps[this.steps.length - 1].active) {
            return false;
        }
        this.steps.some(function (step, index, steps) {
            if (index < steps.length - 1) {
                if (step.active) {
                    // tslint:disable-next-line:triple-equals
                    if (step.name === 'Client Details') {
                        if (clientForm.valid) {
                            step.active = false;
                            step.valid = true;
                            steps[index + 1].active = true;
                            return true;
                        }
                        else {
                            step.hasError = true;
                        }
                    }
                    if (step.name === 'Deceased Details') {
                        if (personalForm.valid) {
                            step.active = false;
                            step.valid = true;
                            steps[index + 1].active = true;
                            return true;
                        }
                        else {
                            step.hasError = true;
                        }
                    }
                    if (step.name === 'Next of Kin') {
                        if (nextOfKinForm.valid) {
                            step.active = false;
                            step.valid = true;
                            steps[index + 1].active = true;
                            return true;
                        }
                        else {
                            step.hasError = true;
                        }
                    }
                }
            }
        });
        console.log(this.details);
    }
    prev() {
        if (this.steps[0].active) {
            return false;
        }
        this.steps.some(function (step, index, steps) {
            if (index !== 0) {
                if (step.active) {
                    step.active = false;
                    steps[index - 1].active = true;
                    return true;
                }
            }
        });
    }
    addAdmission(admission) {
        // console.log(admission);
        this.admissionService.addAdmission(admission).subscribe(admission => {
            console.log(admission);
            this.getAdmissions();
        });
    }
    confirm() {
        // this.steps.forEach(step => step.valid = true);
        // this.confirmed = true;
        // if (this.frmService.valid) {
        if (this.details.id) {
            // this.updateAdmission(this.details);
        }
        else {
            this.details.clientId = this.clientForm.value.clientId;
            this.details.fullName = this.personalForm.value.fullName;
            this.details.documentNumber = this.personalForm.value.documentNumber;
            this.details.gender = this.personalForm.value.gender;
            this.details.description = this.personalForm.value.description;
            this.details.country = this.personalForm.value.country;
            this.details.county = this.personalForm.value.county;
            let dtb = this.personalForm.value.dateOfBirth;
            let tempdtb = new Date(dtb.year, dtb.month - 1, dtb.day);
            this.details.dateOfBirth = tempdtb.toLocaleString().split(',')[0];
            //this.details.dateOfBirth = this.personalForm.value.dateOfBirth;
            this.details.address = 'na';
            this.details.mobileNumber = '000';
            this.details.causeOfDeath = this.personalForm.value.causeOfDeath;
            let dtd = this.personalForm.value.dateOfDeath;
            let tempdtd = new Date(dtd.year, dtd.month - 1, dtd.day);
            this.details.dateOfBirth = tempdtd.toLocaleString().split(',')[0];
            //this.details.dateOfDeath = this.personalForm.value.dateOfDeath;
            this.details.fridgeId = this.personalForm.value.fridge;
            this.details.serviceId = this.personalForm.value.service;
            this.nextOfKin.push(this.nextOfKinForm.value);
            this.details.nextOfKinDetails = this.nextOfKin;
            this.details.description = this.personalForm.value.description;
            this.addAdmission(this.details);
            this.showList = true;
            this.showDetails = false;
            this.clientForm.reset();
            this.personalForm.reset();
            this.nextOfKinForm.reset();
            this.invoiceFrom.reset();
            this.toastrService.success('Saved Successfully!');
        }
        //  }
    }
    generateInvoice(admission) {
        this.showInvoice = true;
        this.showList = false;
        /*  this.invoiceServices.push({'serviceName': admission.serviceName, 'price': 100,
              'fridgeName': admission.fridgeName, 'fridgePrice': 200,'invoiceTotal': 300});*/
        this.invoiceServices.push({
            'serviceName': 'Premium', 'price': 100,
            'fridgeName': 'VM0001', 'fridgePrice': 200, 'invoiceTotal': 300
        });
    }
    cancelInvoice() {
        this.showList = true;
        this.showInvoice = false;
        this.invoiceServices = [];
    }
    editInvoice(admission) {
        this.showList = false;
        this.showDetails = true;
        /*  this.clientForm.controls.clientId.setValue({
              clientId: admission.customer
          });*/
        this.clientForm.controls['clientId'].setValue(admission.customer);
        this.personalForm.controls['fullName'].setValue(admission.fullName);
        this.personalForm.controls['documentNumber'].setValue(admission.documentNumber);
        this.personalForm.controls['gender'].setValue(admission.gender);
        this.personalForm.controls['dateOfBirth'].setValue(admission.dateOfBirth);
        this.personalForm.controls['county'].setValue(admission.county);
        this.personalForm.controls['country'].setValue(admission.country);
        this.personalForm.controls['causeOfDeath'].setValue(admission.causeOfDeath);
        this.personalForm.controls['dateOfDeath'].setValue(admission.dateOfDeath);
        this.personalForm.controls['service'].setValue(admission.service);
        this.getFridgesByServiceId(admission.service);
        this.personalForm.controls['fridge'].setValue(admission.fridge);
        for (let i = 0; i < admission.nextOfKins.length; i++) {
            this.nextOfKinForm.controls['fullName'].setValue(admission.nextOfKins[i].fullName);
            this.nextOfKinForm.controls['documentNumber'].setValue(admission.nextOfKins[i].documentNumber);
            this.nextOfKinForm.controls['gender'].setValue(admission.nextOfKins[i].gender);
            this.nextOfKinForm.controls['county'].setValue(admission.nextOfKins[i].county);
            this.nextOfKinForm.controls['country'].setValue(admission.nextOfKins[i].country);
        }
    }
};
AdmissionComponent.ctorParameters = () => [
