        let bookinginvoice = new _inventory_purchase_bookinginvoice_model__WEBPACK_IMPORTED_MODULE_8__["Bookinginvoice"]();
        bookinginvoice.clientId = invoice.clientId;
        bookinginvoice.invoiceNo = '#INV';
        bookinginvoice.services = this.invoiceServices;
        // console.log(invoice);
        // console.log(bookinginvoice);
        this.addBookingInvoice(bookinginvoice);
    }
    addBookingInvoice(bookinginvoice) {
        this.invoiceService.addBookingInvoice(bookinginvoice).subscribe(resp => {
            if (resp) {
                if (resp.resultCode === '200') {
                    this.toastrService.info(resp.resultMessage, 'Success');
                    this.router.navigate(['/invoice']);
                }
                else {
                    this.toastrService.error(resp.resultMessage, 'Error');
                }
            }
        });
    }
    getSum() {
        let sum = 0;
        for (let i = 0; i < this.invoiceServices.length; i++) {
            sum += this.invoiceServices[i].totalAmount;
        }
        return sum;
    }
};
