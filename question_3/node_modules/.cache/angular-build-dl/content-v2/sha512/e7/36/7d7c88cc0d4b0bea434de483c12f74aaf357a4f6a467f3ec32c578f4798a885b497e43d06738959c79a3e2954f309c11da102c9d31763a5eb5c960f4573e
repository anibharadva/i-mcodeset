let SchemeComponent = class SchemeComponent {
    constructor(fb, toastrService, schemeService, schemetypeService, modalService) {
        this.fb = fb;
        this.toastrService = toastrService;
        this.schemeService = schemeService;
        this.schemetypeService = schemetypeService;
        this.modalService = modalService;
        this.type = 'list';
        this.showDetails = true;
        this.showList = false;
    }
    ngOnInit() {
        this.getSchemes();
        // this.getSchemetypes();
        this.frmScheme = this.fb.group({
            id: null,
            schemeCode: null,
            schemeName: null,
            fee: null,
            active: 0,
            dateCreated: null,
            dateUpdated: null
        });
    }
    getSchemetypes() {
        this.schemetypeService.getSchemetypes().subscribe(schemetypes => this.schemetypes = schemetypes);
    }
    getSchemes() {
        this.schemeService.getSchemes().subscribe(schemes => this.schemes = schemes);
    }
    addScheme(scheme) {
        this.schemeService.addScheme(scheme).subscribe(resp => {
            if (resp) {
                if (resp.resultCode === '200') {
                    this.getSchemes();
                    this.closeModal();
                    this.toastrService.info(resp.resultMessage, 'Success');
                }
                else {
                    this.toastrService.error(resp.resultMessage, 'Error');
                }
            }
        });
    }
    updateScheme(scheme) {
        this.schemeService.updateScheme(scheme).subscribe(resp => {
            if (resp) {
                if (resp.resultCode === '200') {
                    this.getSchemes();
                    this.closeModal();
                    this.toastrService.info(resp.resultMessage, 'Success');
                }
                else {
                    this.toastrService.error(resp.resultMessage, 'Error');
                }
            }
        });
    }
    toggle(type) {
        this.type = type;
    }
    ShowSchemeDetails() {
        this.showDetails = false;
        this.showList = true;
    }
    cancelScheme() {
        this.frmScheme.reset();
        this.schemetypes = [];
        this.showDetails = true;
        this.showList = false;
    }
    onSubmit(scheme) {
        console.log(scheme);
        let scheme_types = [];
        if (this.frmScheme.valid) {
            if (scheme.id) {
                this.updateScheme(scheme);
            }
            else {
                scheme.schemetypes = scheme_types;
                this.addScheme(scheme);
            }
        }
    }
    openModal(modalContent, scheme) {
        if (scheme) {
            this.scheme = scheme;
            this.frmScheme.setValue(scheme);
        }
        else {
            this.scheme = new _scheme_model__WEBPACK_IMPORTED_MODULE_6__["Scheme"]();
        }
        this.modalRef = this.modalService.open(modalContent, { container: '.app' });
        this.modalRef.result.then((result) => {
            this.frmScheme.reset();
        }, (reason) => {
            this.frmScheme.reset();
        });
    }
    closeModal() {
        this.modalRef.close();
    }
};
