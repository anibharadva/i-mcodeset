let SchemetypeComponent = class SchemetypeComponent {
    constructor(fb, toastrService, schemeService, schemetypeService, modalService) {
        this.fb = fb;
        this.toastrService = toastrService;
        this.schemeService = schemeService;
        this.schemetypeService = schemetypeService;
        this.modalService = modalService;
        this.type = 'list';
    }
    ngOnInit() {
        this.getSchemetypes();
        this.getSchemes();
        this.frmSchemetype = this.fb.group({
            id: null,
            schemetypeCode: null,
            schemetypeName: null,
            fee: 0,
            scheme: 0,
            active: 0,
            dateCreated: null,
            dateUpdated: null,
        });
    }
    getSchemes() {
        this.schemeService.getSchemes().subscribe(schemes => this.schemes = schemes);
    }
    getSchemetypes() {
        this.schemetypeService.getSchemetypes().subscribe(schemetypes => this.schemetypes = schemetypes);
    }
    addSchemetype(schemetype) {
        this.schemetypeService.addSchemetype(schemetype).subscribe(schemetype => {
            this.getSchemetypes();
        });
    }
    updateSchemetype(schemetype) {
        this.schemetypeService.updateSchemetype(schemetype).subscribe(schemetype => {
            this.getSchemetypes();
        });
    }
    toggle(type) {
        this.type = type;
    }
    openModal(modalContent, schemetype) {
        if (schemetype) {
            this.schemetype = schemetype;
            this.frmSchemetype.setValue(schemetype);
            this.frmSchemetype.controls['scheme'].setValue(schemetype.scheme.id);
        }
        else {
            this.schemetype = new _schemetype_model__WEBPACK_IMPORTED_MODULE_7__["Schemetype"]();
        }
        this.modalRef = this.modalService.open(modalContent, { container: '.app' });
        this.modalRef.result.then((result) => {
            this.frmSchemetype.reset();
        }, (reason) => {
            this.frmSchemetype.reset();
        });
    }
    closeModal() {
        this.modalRef.close();
    }
    onSubmit(schemetype) {
        if (this.frmSchemetype.valid) {
            if (schemetype.id) {
                this.updateSchemetype(schemetype);
            }
            else {
                this.addSchemetype(schemetype);
            }
            this.modalRef.close();
        }
    }
};
