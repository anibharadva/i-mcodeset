let ServicesComponent = class ServicesComponent {
    constructor(fb, toastrService, mileleServices, membershipService, modalService) {
        this.fb = fb;
        this.toastrService = toastrService;
        this.mileleServices = mileleServices;
        this.membershipService = membershipService;
        this.modalService = modalService;
        this.type = 'list';
    }
    ngOnInit() {
        this.getServiceTypes();
        this.getUoms();
        this.getServices();
        this.frmService = this.fb.group({
            id: null,
            serviceId: null,
            serviceCode: [null, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].maxLength(100)])],
            serviceName: [null, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_3__["Validators"].maxLength(50)])],
            price: null,
            unit: null,
            serviceTypeName: null,
            serviceType: 0,
            unitId: 0,
            quantity: 0,
            totalAmount: 0
        });
    }
    getServices() {
        this.mileleServices.getServices().subscribe(services => this.services = services);
    }
    getCountries() {
        this.membershipService.getCountries().subscribe(countries => this.countries = countries);
    }
    getCounties() {
        this.membershipService.getCounties().subscribe(counties => this.counties = counties);
    }
    getServiceTypes() {
        this.mileleServices.getServiceTypes().subscribe(servicetypes => this.servicetypes = servicetypes);
    }
    getUoms() {
        this.mileleServices.getUoms().subscribe(uoms => this.uoms = uoms);
    }
    addService(service) {
        console.log(service);
        this.mileleServices.addService(service).subscribe(service => {
            this.getServices();
        });
    }
    updateService(service) {
        this.mileleServices.updateService(service).subscribe(service => {
            this.getServices();
        });
    }
    toggle(type) {
        this.type = type;
    }
    openModal(modalContent, service) {
        console.log(service);
        if (service) {
            this.service = service;
            this.frmService.setValue(service);
        }
        else {
            this.service = new _services_service_model__WEBPACK_IMPORTED_MODULE_2__["Service"]();
        }
        this.modalRef = this.modalService.open(modalContent, { container: '.app' });
        this.modalRef.result.then((result) => {
            this.frmService.reset();
        }, (reason) => {
            this.frmService.reset();
        });
    }
    closeModal() {
        this.modalRef.close();
    }
    onSubmit(service) {
        console.log(service);
        if (this.frmService.valid) {
            if (service.id) {
                this.updateService(service);
            }
            else {
                this.addService(service);
            }
            this.modalRef.close();
        }
    }
};
